{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-undef": "warn",
      "camelcase": "off",
      "no-unused-vars": "warn",
      "eqeqeq": "warn",
      "array-callback-return": "off",
      "prefer-regex-literals": "off"
    }
}
